{
  "name": "@distube/yt-dlp",
  "version": "1.0.2",
  "description": "yt-dlp plugin for DisTube.js.org",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --write **/*.{js,ts,json,yml,yaml,md}",
    "build": "tsc",
    "build:check": "tsc --noEmit --incremental false",
    "npm:publish": "npm publish --access public",
    "postinstall": "node script/download",
    "postinstall:dev": "npm run build && node script/download && husky install",
    "prepublishOnly": "node script/prepublishOnly.dev",
    "postpublish": "node script/postpublish.dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/distubejs/yt-dlp.git"
  },
  "keywords": [
    "distube",
    "plugin",
    "yt-dlp",
    "youtube-dl",
    "discord.js",
    "discord-music-bot"
  ],
  "author": "Skick",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/distubejs/yt-dlp/issues"
  },
  "homepage": "https://github.com/distubejs/yt-dlp#readme",
  "dependencies": {
    "dargs": "^7.0.0",
    "execa": "^5.1.1",
    "mkdirp": "^1.0.4",
    "petitio": "^1.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@discordjs/opus": "^0.7.0",
    "@types/mkdirp": "^1.0.2",
    "commitlint": "^16.1.0",
    "discord.js": "^13.6.0",
    "distube": "^3.3.2",
    "eslint": "^8.8.0",
    "eslint-config-distube": "^1.6.3",
    "fromentries": "^1.3.2",
    "husky": "^7.0.4",
    "nano-staged": "^0.5.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5"
  },
  "peerDependencies": {
    "distube": "^3.1.0"
  },
  "nano-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{js,json,yml,yaml,md}": "prettier --write"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
